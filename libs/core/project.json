{
  "root": "libs/core",
  "sourceRoot": "libs/core/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/v9helper",
        "tsConfig": "libs/core/tsconfig.lib.json",
        "packageJson": "libs/core/package.json",
        "main": "libs/core/src/index.js",
        "assets": ["libs/core/*.md"]
      }
    },
    "build:cli": {
      "executor": "@nrwl/workspace:run-commands",
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ],
      "options": {
        "commands": [{ "command": "chmod -R 755 dist/libs/v9helper" }]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/core/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/core"],
      "options": {
        "jestConfig": "libs/core/jest.config.js",
        "passWithNoTests": true
      }
    },
    "buildcore": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/core/dist",
        "tsConfig": "libs/core/tsconfig.lib.json",
        "packageJson": "libs/core/package.json",
        "main": "libs/core/src/lib/core.js"
      }
    },
    "debug": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "core:buildcore"
      }
    }
  },
  "tags": []
}
